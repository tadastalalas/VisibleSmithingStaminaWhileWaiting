	<strings>
		<string id="EuYdW1aH89JPd" text="Beklerken Görünür Demircilik Dayanıklılığı" />
		<string id="Zqsbdz6MIc9Ex" text="Ana kahraman dayanıklılığı %100'e ulaştı" />
		<string id="OcXCbctwrryDV" text="Demircilik dayanıklılığı bildirimlerinizin nasıl gösterilmesini istediğinizi seçin:" />
		<string id="2wemouR6uduxG" text="Ek seçenekler:" />
		<string id="PDnUz0EnsVJyH" text="Olay günlüğü bildirimi" />
		<string id="3UWpkfOEaAIzL" text="Ekranın sol alt köşesindeki günlükte demircilik dayanıklılığı bildirimini göster." />
		<string id="4gwYs6wqtoMx9" text="Orta ekran bildirimi" />
		<string id="F4b8RhAxgtqAy" text="Ekranın üst orta kısmında demircilik dayanıklılığı bildirimini göster." />
		<string id="W9KhHjSq0EHDc" text="Yuvarlak açılır bildirim" />
		<string id="wY5QrbROdk7M5" text="Ekranın sağ tarafında demircilik dayanıklılığı bildirimini yuvarlak bir açılır pencere olarak göster." />
		<string id="GK78CZFIe9Dti" text="Dayanıklılık dolduğunda beklemeyi bırakın" />
		<string id="yOQyJJBraA767" text="Ana kahramanın dayanıklılığı %100'e ulaştığında kahraman beklemeyi bırakacaktır." />
		<string id="sO6sGMmSdvZQW" text="Seyahat ederken dayanıklılığınızı yenileyin" />
		<string id="Z1K0iP4UAKq6c" text="Tüm kahraman grubu üyeleri seyahat ederken demircilik dayanıklılıklarını yenileyecekler. 3 gün = %100 dayanıklılık." />
		<string id="6qMBxn8agWxK2" text="Seyahat ederken bildirimleri göster" />
		<string id="zCzem6NK44Yde" text="Kahraman seyahat ederken demircilik dayanıklılığının geri kazanılması bildirimlerini göster. Bunun çalışması için [Seyahat ederken dayanıklılığı yenile] seçeneğinin etkinleştirilmesi gerekir." />
		<string id="zdTrD1TxVpbQY" text="Ana kahraman dayanıklılığı {percent}%" />
		<string id="6oTamV4M8XdZO" text="[Kasaba] Mevcut dayanıklılık yüzdesini göster" />
		<string id="WtDruY7sQcxsx" text="Şehirde beklerken her saat güncel demircilik dayanıklılık yüzdesini göster." />
		<string id="ErodgkpkVhxKk" text="[Seyahat] Mevcut dayanıklılık yüzdesini göster" />
		<string id="jLu2I19JjbY84" text="Dünya üzerinde seyahat ederken her saat güncel demircilik dayanıklılık yüzdesini göster." />
